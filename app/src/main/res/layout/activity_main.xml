<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="pl.com.hit_kody.www.exterionalstorage.MainActivity">

    <TextView
        android:text="@string/TextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="58dp"
        android:id="@+id/textView2" />

    <Button
        android:text="@string/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="81dp"
        android:layout_marginTop="31dp"
        android:id="@+id/button"
        android:layout_x="66dp"
        android:layout_y="137dp" />

    <Button
        android:text="@string/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button2"
        android:layout_x="232dp"
        android:layout_y="29dp"
        android:layout_alignBottom="@+id/textView2"
        android:layout_toEndOf="@+id/button"
        android:layout_marginStart="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/TextView"
        android:id="@+id/textView"
        android:layout_marginTop="104dp"
        style="@style/Wielki_napis"
        android:layout_x="102dp"
        android:layout_y="334dp"
        android:layout_below="@+id/button"
        android:layout_toStartOf="@+id/button2"
        android:layout_marginEnd="11dp" />
</RelativeLayout>
